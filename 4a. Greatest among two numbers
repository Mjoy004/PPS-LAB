#include <stdio.h>

int main() {
    int num1, num2;
    
    // Prompt the user to enter two numbers
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);
    
    // Compare the two numbers and print the result
    if (num1 > num2) {
        printf("%d is the greatest.\n", num1);
    } else if (num2 > num1) {
        printf("%d is the greatest.\n", num2);
    } else {
        printf("Both numbers are equal.\n");
    }
    
    return 0;
}
